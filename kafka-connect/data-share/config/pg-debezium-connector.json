{
  "name": "my-test-connector",
  "config": {
    "plugin.name": "pgoutput",
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "database.hostname": "kube0",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "false",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "database.port": "30000",
    "database.user": "sachin",
    "database.password": "123456",
    "database.dbname" : "mydb",
    "database.server.name": "my-topics-07-partitioned",
    "provide.transaction.metadata": "true",
    "table.include.list": "public.user_tbl,public.user_address_tbl",
    "message.key.columns": "public.user_tbl:id;public.user_address_tbl:user_id",
    "transforms": "unwrap,ExtractKey,FlattenStruct",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.ExtractKey.type": "org.apache.kafka.connect.transforms.ValueToKey",
    "transforms.ExtractKey.fields": "id",
    "transforms.FlattenStruct.type" : "org.apache.kafka.connect.transforms.ExtractField$Key",
    "transforms.FlattenStruct.field": "id"
  }
}