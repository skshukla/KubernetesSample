{
  "name": "my-test-connector",
  "config": {
    "plugin.name": "pgoutput",
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "database.hostname": "kube0",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "false",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "database.port": "30000",
    "database.user": "sachin",
    "database.password": "123456",
    "database.dbname" : "mydb",
    "database.server.name": "my-topics",
    "provide.transaction.metadata": "true",
    "table.include.list": "public.kafka_connect_test_table,public.user_tbl,public.user_address_tbl",
    "transforms": "ExtractKey",
    "transforms.ExtractKey.type": "org.apache.kafka.connect.transforms.ValueToKey",
    "transforms.ExtractKey.fields": "id"

  }
}